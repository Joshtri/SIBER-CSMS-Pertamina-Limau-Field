  <script>
    //FUNCTION CARD verifikasi user //
    function showCardVerifikasiUser(){
      // card-verifikator-user
      document.getElementById("card-verifikator-user").style.display = "block";
      document.getElementById("card-verifikator-user").classList.add("animate__zoomIn");
      document.getElementById("card-verifikator-user").classList.remove("animate__zoomOut");
      document.getElementById("card-verifikator-user").classList.remove("hidden");
    }


    function hideCardVerifikasiUser(){
      document.getElementById("card-verifikator-user").classList.add("hidden");
      document.getElementById("card-verifikator-user").classList.remove("animate__zoomIn");
      document.getElementById("card-verifikator-user").classList.add("animate__zoomOut");
